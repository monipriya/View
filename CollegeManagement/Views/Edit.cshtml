@model CollegeDetail.Entity.Detail

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>
@using (Html.BeginForm("Update", "Package", FormMethod.Post))
{
    @Html.HiddenFor(Model => Model.collegeCode)
    <table cellpadding="10">
        <tr>
            <td>
                @Html.Label("College Name")
            </td>
            <td>
                @Html.EditorFor(Model => Model.collegeName)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("collegerank")
            </td>
            <td>
                @Html.EditorFor(Model => Model.collegeRank)
            </td>
        </tr>
        <tr>
            <td>
                @Html.ActionLink("Cancel", "Index")
            </td>
            <td>
                <input type="submit" value="Update" />
            </td>
        </tr>
    </table>
}


